        <command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="10" aliases="none">
          <maml:name>JarFileRegex</maml:name>
          <maml:Description>
            <maml:para>The regular expression that is used when looking for jar files.</maml:para>
